{
  "name": "hylia",
  "version": "0.5.1",
  "description": "A simple Eleventy starter kit to help you have a blog of your own",
  "main": "index.js",
  "dependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@tbranyen/jsdom": "^13.0.0",
    "concurrently": "^5.0.2",
    "dotenv": "^8.2.0",
    "encoding": "^0.1.12",
    "html-minifier": "^4.0.0",
    "json-to-scss": "^1.4.0",
    "markdown-it-classy": "^0.2.0",
    "netlify-lambda": "^1.6.3",
    "node-fetch": "^2.6.0",
    "sass": "^1.25.0",
    "semver": "^7.1.1",
    "slugify": "^1.3.6",
    "stalfos": "github:hankchizljaw/stalfos#c8971d22726326cfc04089b2da4d51eeb1ebb0eb",
    "twitter": "^1.7.1"
  },
  "devDependencies": {
    "@erquhart/rollup-plugin-node-builtins": "^2.1.5",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "bl": "^4.0.0",
    "chokidar-cli": "^2.1.0",
    "cross-env": "^7.0.0",
    "fast-xml-parser": "^3.16.0",
    "make-dir-cli": "^2.0.0",
    "minimist": "^1.2.0",
    "node-jq": "^1.11.0",
    "prettier": "^1.19.1",
    "rollup": "^1.29.1",
    "rollup-plugin-json": "^4.0.0"
  },
  "scripts": {
    "postinstall": "netlify-lambda install",
    "sass:tokens": "npx json-to-scss src/_data/tokens.json src/scss/_tokens.scss",
    "sass:process": "npm run sass:tokens && sass src/scss/global.scss src/_includes/assets/css/global.css --style=compressed",
    "cms:precompile": "make-dir dist/admin && nunjucks-precompile src/_includes > dist/admin/templates.js -i \"\\.(njk|css|svg)$\"",
    "cms:bundle": "rollup --config",
    "start": "concurrently \"npm run sass:process -- --watch\" \"npm run cms:bundle -- --watch\" \"chokidar \\\"src/_includes/**\\\" -c \\\"npm run cms:precompile\\\"\" \"npm run serve\"",
    "serve": "cross-env ELEVENTY_ENV=development npx eleventy --serve",
    "serve:lambda": "netlify-lambda serve _lambda",
    "build:lambda": "netlify-lambda build _lambda",
    "production": "npm run sass:process && npm run cms:precompile && npm run cms:bundle && npm run build:lambda && npx eleventy",
    "post:new": "node _task/node-trello-contents.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frontendweekly/hylia.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frontendweekly/hylia/issues"
  },
  "homepage": "https://github.com/frontendweekly/hylia#readme"
}
